{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "identity": {
        "ClusterId": "identityCluster",
        "Match": {
          "Path": "/identity/{**catch-all}"
        }
      },
      "order": {
        "ClusterId": "orderCluster",
        "Match": {
          "Path": "/order/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identityCluster": {
        "Destinations": {
          "identityDestination": {
            "Address": "http://localhost:5240/"
          }
        }
      },
      "orderCluster": {
        "Destinations": {
          "orderDestination": {
            "Address": "http://localhost:5095/"
          }
        }
      }
    }
  }
}
