{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ElasticsearchOptions": {
    "NodeUris": [
      "http://localhost:9200"
    ],
    "DataStream": {
      "Type": "logs",
      "Dataset": "bugbucks-localhost",
      "Namespace": "demo"
    },
    "BootstrapMethod": "Failure",
    "BufferOptions": {
      "ExportMaxConcurrency": 10
    }
  }
}
